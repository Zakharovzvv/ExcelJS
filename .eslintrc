{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "parser": "babel-eslint",
  "plugins": ["jest"],
  "extends": ["airbnb/base"],
  "rules": {
    "no-plusplus": "off",
    "no-tabs": "off",
    "prefer-destructuring": "off",
    "indent": [2,"tab"],
    "max-len": ["error",150,{"ignoreUrls": true}],
    "no-alert": "off",
    "no-console": "off",
    "import/prefer-default-export": "off",
    "class-methods-use-this": "off",
    "no-param-reassign": "off"
  }
}
